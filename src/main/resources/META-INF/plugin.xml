<idea-plugin>
    <actions>
        <action
                class="com.eggnstone.jetbrainsplugins.dartformat.Plugin"
                id="com.eggnstone.jetbrainsplugins.dartformat.Plugin">
            <keyboard-shortcut
                    first-keystroke="control alt COMMA"
                    keymap="$default"/>
            <!-- รถ <keyboard-shortcut first-keystroke="ctrl alt #10000d6" /> -->
        </action>
    </actions>
    <depends>com.intellij.modules.platform</depends>
    <description>A better dart format. Because it's configurable.</description>
    <id>com.eggnstone.jetbrainsplugins.DartFormat</id>
    <name>DartFormat</name>
    <vendor email="support@eggnstone.com" url="https://www.eggnstone.com">egg 'n stone</vendor>
</idea-plugin>
